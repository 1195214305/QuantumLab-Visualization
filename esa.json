{
  "version": 1,
  "routes": [
    {
      "src": "/api/ai/chat",
      "dest": "/functions/ai/chat.js"
    },
    {
      "src": "/api/simulate",
      "dest": "/functions/simulate.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ],
  "build": {
    "output": "frontend/dist"
  },
  "functions": {
    "directory": "functions",
    "runtime": "edge"
  }
}
